QAAA,aAAA,EAAA;AAFA;AAVA,KAAA;AAAA;AADA,CAAA","sourcesContent":["<template>\n    <form ref=\"form\">\n        <v-text-field\n            v-model=\"loginUser.email\"\n            :rules=\"emailRules\"\n            label=\"E-mail\"\n            required\n            color=\"#039629\"\n        ></v-text-field>\n        <v-text-field\n            v-model=\"loginUser.password\"\n            :append-icon=\"showPassword ? svg.visibility : svg.visibilityOff\"\n            :type=\"showPassword ? 'text' : 'password'\"\n            name=\"input-10-1\"\n            label=\"Password\"\n            hint=\"At least 6 char acters\"\n            counter\n            @click:append=\"showPassword = !showPassword\"\n            color=\"#039629\"\n        ></v-text-field>\n\n        <!-------------------------  FORM ERRORS  ------------------->\n        <div class=\"subtitle1 text-center text-capitalize red--text\">\n            <span>¿No tienes cuenta? <router-link class=\"cyan--text\" to=\"/signup\">¡Registrate Aqui!</router-link></span>\n        </div>\n        <!-------------------------  END FORM ERRORS ------------------->\n\n        <div class=\"mt-5\">\n            <v-btn type=\"submit\" class=\"mr-4\" color=\"#039629\" elevation=\"0\" dark>\n                Entrar\n            </v-btn>\n            <v-btn color=\"#039629\" elevation=\"0\" dark>\n                Limpiar\n            </v-btn>\n        </div>\n    </form>\n</template>\n<script>\n// MIXINS\n// SVG ICONS\nimport { mdiEyeOutline, mdiEyeOffOutline  } from '@mdi/js';\n\n//VUEX\nexport default {\n    data: () => ({\n        showPassword: false,\n        loginUser: {\n            email: '',\n            password: ''\n        },\n        emailRules: [\n            v => !!v || 'E-mail is required',\n            v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\n        ],\n        svg: {\n            visibility: mdiEyeOutline,\n            visibilityOff: mdiEyeOffOutline,\n        },\n    })\n}\n</script>"],"sourceRoot"