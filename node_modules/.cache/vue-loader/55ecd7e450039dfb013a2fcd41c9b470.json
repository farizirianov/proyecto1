\">\n        </v-list-item-avatar>\n\n        <v-list-item-content>\n          <v-list-item-title>{{user.firstName}}</v-list-item-title>\n          <v-list-item-subtitle>{{user.lastName}}</v-list-item-subtitle>\n        </v-list-item-content>\n      </v-list-item>\n\n      <v-divider></v-divider>\n\n      <v-list-item>\n        <v-list-item-icon>\n          <v-icon>{{ svg.logout }}</v-icon>\n        </v-list-item-icon>\n\n        <v-list-item-content>\n          <v-list-item-title>Salir</v-list-item-title>\n        </v-list-item-content>\n      </v-list-item>\n    </v-list>\n    </v-card>\n  </v-menu>\n</template>\n\n<script>\n  import api from '../../services/api'\n\n  import {mdiLogout} from '@mdi/js'\n\n  export default {\n    props: ['idUser'],\n    data: () => ({\n      miniVariant: false,\n      dialog: false,\n      user: {},\n      svg: {\n        logout: mdiLogout\n      }\n    }),\n    methods: {\n      async getUser() {\n        const dataUser = await api.getUser(this.idUser)\n        this.user = dataUser.data\n      }\n    },\n    created() {\n      this.getUser()\n    }\n  }\n</script>"]}]}