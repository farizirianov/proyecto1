plate>\n\n<script>\nimport VueJwtDecode from \"vue-jwt-decode\"\n\nexport default {\n  data: () =>({\n    user: {}\n  }),\n  methods: {\n    getUserDetails() {\n      try {\n        const token = localStorage.getItem(\"jwt\")\n        let decoded = VueJwtDecode.decode(token)\n        this.user = decoded        \n      } catch (e) {\n        console.log({message: e})        \n      }\n    },\n    logUserOut() {\n      localStorage.removeItem(\"jwt\")\n      this.$router.push(\"/\")\n    }\n  },\n  created() {\n    this.getUserDetails();\n  }\n}\n</script>"]}]}