ow align=\"center\" class=\"spacer\" no-gutters>\n              <v-col cols=\"3\" class=\"center pa-0\">\n                  <v-img max-height=\"65\" max-width=\"65\" :src=\"insignia.idInsignia.icon\"></v-img>\n              </v-col>\n\n              <v-col class=\"center pa-0\">\n                <v-list-item-content>\n                  <v-list-item-title>\n                    {{insignia.idInsignia.title}}\n                  </v-list-item-title>\n                  <v-list-item-subtitle>\n                    {{insignia.idInsignia.description}}\n                  </v-list-item-subtitle>\n                  <v-row align=\"center\" class=\"spacer\" no-gutters>\n                    <v-col class=\"pa-0\" align=\"center\">\n                      <v-progress-linear\n                        height=\"25\"\n                        rounded\n                        color=\"#039629\"\n                        class=\"primario\"\n                      >\n                        <strong>{{ Math.ceil(progresBar(insignia.progres,insignia.idInsignia.objective)) }}%</strong>\n                      </v-progress-linear>\n                    </v-col>\n                    <v-col cols=\"2\" class=\"pa-0\" align=\"center\">\n                      {{insignia.progres}}/{{insignia.idInsignia.objective}}\n                    </v-col>\n                  </v-row>\n                </v-list-item-content>\n              </v-col>\n            </v-row>\n          </v-card>\n          </v-list-item>\n        </v-list-item-group>\n      </v-list>\n    <Navbar></Navbar>\n  </v-main>\n</template>\n\n<script>\n  import api from '../services/api'\n  import Navbar from '@/components/Layout/NavBar'\n  import SearchBar from '@/components/Layout/SearchBar'\n\n  export default {\n    props: ['idUser'],\n    components: {\n      Navbar,\n      SearchBar\n    },\n    data: () => ({  \n      title: 'Insignia',    \n       insignia: 1,\n       insignias: []\n    }),\n    methods: {\n      async mostrar() {\n        try {\n          const insig = await api.getAllInsigniasByUser(this.idUser)\n          this.insignias = insig.data\n          console.log(insig)\n        } catch (e) {\n          console.log({message: e})\n        }\n      },\n      progresBar(pro, obj) {\n        return pro/obj*100\n      }\n    },\n    created() {\n      this.mostrar()\n    }\n  }\n</script>"]}]}